<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.8/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-sRIl4kxILFvY47J16cr9ZwB07vP4J8+LH7qKQnuqkuIAvNWLzeN8tE5YBujZqJLB" crossorigin="anonymous">
</head>

<body>
    <h1>Food Entries Listing</h1>
    <table class="table">
        <thead>
            <tr>
                <th>When</th>
                <th>Food Name</th>
                <th>Calories</th>
                <th>Meal</th>
                <th>Tags</th>
            </tr>
        </thead>
        <tbody>
            <% for (let foodEntry of foodEntries) { %>
                <tr>
                    <td><%= foodEntry.dateTime %></td>
                    <td><%= foodEntry.foodName %></td>
                    <td><%= foodEntry.calories %></td>
                    <td><%= foodEntry.meal %></td>
                    <td>
                        <%= JSON.parse(foodEntry.tags) %>
                    </td>
                </tr>
            <% } %>

        </tbody>
    </table>



</body>

</html>